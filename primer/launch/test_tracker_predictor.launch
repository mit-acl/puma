<launch>

	<arg name="quad" default="SQ01s"/>
	<arg name="goal_topic" default="goal"/>
	<arg name="cloud_topic" default="camera/cloud" />   


	<node ns="$(arg quad)" name="test_tracker_predictor" pkg="primer" type="test_tracker_predictor" output="screen" clear_params="false"> 
	<rosparam file="$(find primer)/matlab/casadi_generated_files/params_casadi_prediction.yaml" command="load"/>
	<rosparam file="$(find primer)/param/predictor.yaml" command="load"/>
	<remap from="~cloud" to="$(arg cloud_topic)"/>
	</node>

	<!-- <node pkg="rviz" type="rviz" name="rviz" args="-d $(find primer)/rviz_cfgs/panther.rviz"/> -->

</launch>
